<h3>ChildLifecycleComponent</h3>



@code {
    
    public override Task SetParametersAsync(ParameterView parameters)
    {
        Console.WriteLine("ChildLifecycleComponent ---- SetParametersAsync");
        return base.SetParametersAsync(parameters);
    }

    protected override void OnInitialized()
    {
        Console.WriteLine("ChildLifecycleComponent ---- OnInitialized");
        base.OnInitialized();
    }

    protected override void OnParametersSet()
    {
        Console.WriteLine("ChildLifecycleComponent ---- OnParametersSet");
        base.OnParametersSet();
    }


    protected override bool ShouldRender()
    {
        Console.WriteLine("ChildLifecycleComponent ---- ShouldRender");
        return base.ShouldRender();
    }


    protected override void OnAfterRender(bool firstRender)
    {
        Console.WriteLine("ChildLifecycleComponent ---- OnAfterRender");
        base.OnAfterRender(firstRender);
    }
}
